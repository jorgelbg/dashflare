    "@types/ngeohash": "^0.6.2",
    "@types/ua-parser-js": "^0.7.33",
    "country-list": "^2.2.0",
    "file-type": "^16.5.4",
    "inbound": "0.0.6",
    "latlon-geohash": "^2.0.0",
    "logfmt": "^1.3.2",
    "ngeohash": "^0.6.3",
    "remove": "^0.1.5",
